package player;

public class PLAY {
   private int age;
   private char symbol;
    private String name;
   private String email;
    private String number;
   
    public void setInfoOfplayer(int age, char symbol, String name, String email, String number)
    {
       this.age=age;
       this.symbol=symbol;
       this.name=name;
       this.email=email;
       this.number=number;
    }
    public void setPlayerNameAndage(String name, int age){
        this.name=name;
        this.age=age;
    }
    public void setPlayerNameAndage(String name, int age, String email, char symbol){
        this.name=name;
        this.age=age;
        this.email=email;
        this.symbol=symbol;
    }
    public void setPlayerNameAndage(String name, char symbol){
        this.name=name;
        this.symbol=symbol;
    }
    public String printname(){
        return this.name;
    }
    public char printsymbol(){
      return this.symbol;
    } 
    public void getInfo(){
        System.out.println("NAME= "+ this.name);
        System.out.println( "EMAIL= "+ this.email);
        System.out.println("NUMBER="+ this.number);
        System.out.println("AGE= "+ this.age);
        System.out.println("SYMBOL= "+ this.symbol);
    }
}
